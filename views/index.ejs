<main class="mb-5">
    <% if(!locals.videos){
    %>

        <div class="form-container">
            <h1 class="text-white text-center fw-bold"><%= __('h1') %></h1>
            <h3 class="text-white text-center mt-4"><%= __('h2') %></h3>

            <form
                    method="post" action="/"
                    class="w-100 d-flex flex-column flex-sm-row align-items-sm-end justify-content-center mt-5">

                <div id="input_container">

                    <% if(locals.error === 0){ %>
                        <span id="noti" class="text-white"> <%= __('error') %></span>
                    <%
                    }else if(locals.error === 1){
                    %>
                        <span id="noti" class="text-white"> <%= __('timeout') %></span>
                    <%
                    } %>
                    <div class="input-group">
                        <input id="ip_link" type="text" class="form-control" name="url" required
                               autocomplete="off"
                               placeholder="<%= __('placeholder') %>" aria-label="Username"
                               aria-describedby="basic-addon1">
                        <input type="hidden" id="ip_lang" name="lang" value="<%= __('lang') %>">
                        <span id="btn_paste" class="input-group-text fw-bold px-4" style="color: #0098ff !important;">
                        <img class="img-button mx-1" src="/images/paste.png" style="height: 1.1rem; width: 1.1rem"
                             alt="paste">
                        <%= __('paste') %></span>
                    </div>
                </div>

                <div class="mt-2 mt-sm-0">
                    <button id="btn_download" type="submit"
                            class="btn bg-white text-dark p-2 p-sm-3 mx-sm-2 ">
                        <img class="img-button mx-1" src="/images/downloading.png" style="height: 1.3rem; width: 1.3rem"
                             alt="downloading">
                        <%= __('download') %></button>
                    <div id="loading"
                         style="display:none;"
                         class="btn w-100 bg-white text-dark p-2 p-sm-4 mx-sm-2">
                        <div class="spinner-border spinner-border-sm text-dark" role="status">
                            <span class="visually-hidden">Loading...</span>
                        </div>
                    </div>
                </div>
            </form>
        </div>

        <div class="des-container" style="margin-top: 25rem;">
            <h2 class=" mb-2 fw-bold text-center fw-bold"><%= __('h1_title') %></h2>
            <p class=" mt-4"> <%= __('h1_content') %></p>

            <div class="row g-2 g-sm-2 my-3">
                <div class="col-12 col-md-5 mb-3 mb-sm-0 d-flex justify-content-center">
                    <img src="/images/card1.webp"
                         class="img-fluid rounded" alt="...">
                </div>
                <div class="col-12 col-md-7">
                    <h2 class=" fw-bold"><%= __("card1_title") %></h2>
                    <p class=""><%= __("card1_content") %></p>
                </div>
            </div>

            <div class="row g-2 g-sm-2 my-3">
                <div class="col-12 col-md-7 mb-3 mb-md-0 order-2 order-md-1">
                    <h2 class=" fw-bold"><%= __("card2_title") %></h2>
                    <p class=""><%= __("card2_content") %></p>
                </div>
                <div class="col-12 col-md-5 order-1 order-md-2 d-flex justify-content-center">
                    <img src="/images/card2.webp"
                         class="img-fluid rounded" alt="...">
                </div>
            </div>

            <h2 class=" mt-md-5 fw-bold"><%= __('h2_title1') %></h2>
            <ul class="list-group px-4">
                <% __('h2_content1').forEach(function(item) { %>
