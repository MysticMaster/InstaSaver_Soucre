                <a href="/<%= __('lang') %>"
                   style="background-color: #0098ff;"
                   class="w-100 justify-content-center text-white align-items-center btn mt-3 btn fs-5 p-2 p-sm-2 ">
                    <%= __('back') %>
                </a>
            </div>
        </div>

    <%
