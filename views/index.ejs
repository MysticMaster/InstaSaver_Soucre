<script>
    const ip_link = document.getElementById("ip_link"), btn_paste = document.getElementById("btn_paste"),
        btn_download = document.getElementById("btn_download"), loading = document.getElementById("loading"),
        noti = document.getElementById("noti"), input_container = document.getElementById("input_container");

    if (input_container) {
        input_container.addEventListener("click", () => {
            if (noti) {
                noti.innerText = ""
            }
        })
    }

    if (btn_paste) {
        btn_paste.addEventListener("click", async () => {
            try {
                ip_link.value = await navigator.clipboard.readText()
            } catch (n) {
                console.error("Error pasting from clipboard:", n)
            }
        });
    }
    if (btn_download) {
        btn_download.addEventListener("click", function () {
            btn_download.style.display = "none", loading.style.display = "block", "" === ip_link.value && (btn_download.style.display = "block", loading.style.display = "none")
        });
    }
</script>

